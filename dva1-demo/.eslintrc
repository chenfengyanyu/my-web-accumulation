{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "rules": {
    "spaced-comment": [0],
    "no-unused-vars": [0],
    "no-unused-expressions": [0],
    "no-empty": [0],
    "no-console": [0],
    "no-param-reassign": [0],
    "prefer-const": [0],
    "no-extend-native": [0],
    "func-names": [0],
    "react/wrap-multilines": [0],
    "react/no-multi-comp": [0],
    "no-constant-condition": [0],
    "react/jsx-no-bind": [0],
    "react/prop-types": [0],
    "arrow-body-style": [0],
    "react/prefer-stateless-function": [0],
    "semi": [0],
    "global-require": [0],
    "no-shadow": [0],
    "no-useless-computed-key": [0],
    "no-underscore-dangle": [0],
    "max-len": [0],
    "no-else-return": [0],
    "consistent-return": [0],
    "no-restricted-syntax": [0],
    "new-cap": [0],
    "no-unneeded-ternary": [0]
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
    "jsx": true
  },
  "globals": {
    "$": true
  }
}
